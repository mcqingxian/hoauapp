package com.hoau.hoauapp.vo;

import com.hoau.hoauapp.entity.Location;

public class DepartmentListVO {
	
	/**
	 * 网点名称
	 */
	private String name;
	/**
	 * 网点地址
	 */
	private String address;
	/**
	 * 是否定日达
	 */
	private boolean DDAvailable;
	/**
	 * 是否可发货
	 */
	private boolean isShipment;
	/**
	 * 是否可送货
	 */
	private boolean isDelivery;
	/**
	 * 是否可自提
	 */
	private boolean isPickUp;
	/**
	 * 距离
	 * */
	private String directDistance;
	/**
	 * 网点经纬度
	 */
	private Location location;
	/**
	 * 网点代码
	 */
	private String logistCode;
	/**
	 * 电话
	 */
	private String telephone;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	
	public boolean isDDAvailable() {
		return DDAvailable;
	}
	public void setDDAvailable(boolean dDAvailable) {
		DDAvailable = dDAvailable;
	}
	public String getDirectDistance() {
		return directDistance;
	}
	public void setDirectDistance(String directDistance) {
		this.directDistance = directDistance;
	}
	public Location getLocation() {
		return location;
	}
	public void setLocation(Location location) {
		this.location = location;
	}
	public String getTelephone() {
		return telephone;
	}
	public void setTelephone(String telephone) {
		this.telephone = telephone;
	}
	public String getLogistCode() {
		return logistCode;
	}
	public void setLogistCode(String logistCode) {
		this.logistCode = logistCode;
	}
	public boolean isShipment() {
		return isShipment;
	}
	public void setShipment(boolean isShipment) {
		this.isShipment = isShipment;
	}
	public boolean isDelivery() {
		return isDelivery;
	}
	public void setDelivery(boolean isDelivery) {
		this.isDelivery = isDelivery;
	}
	public boolean isPickUp() {
		return isPickUp;
	}
	public void setPickUp(boolean isPickUp) {
		this.isPickUp = isPickUp;
	}
}
